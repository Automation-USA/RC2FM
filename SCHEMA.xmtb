    <Field id="47" dataType="Text" fieldType="Calculated" name="sms_message">
      <Calculation table="RC2FM_LOGS">// s:19:"http://360works.com"; 
// n:42;

If ( type = "SMS" ; 

	Let (  [ 
    		trimmed = Left ( subject ; Length ( subject ) - 1 ) ; // remove semicolon
    		p1 = Position ( trimmed ; ":" ; 1 ; 1 ) ; // first colon, should always = 2
    		p2 = Position ( trimmed ; ":" ; 1 ; 2) ; // second colon, only for string types
   		 type = Left ( trimmed ; p1 -1 ) ;
    		v2 = Middle ( trimmed ; p1 + 1; p2-p1);
    		v3 = Middle ( trimmed ; p2 + 1 ; Length(trimmed) - p2 )
	] ;
    		Case ( 
       		IsEmpty( subject ) ; // empty input
           	 "" ;
        		type = "N" ;  // null
           	 "" ;
        		type = "b" ; // boolean
			GetAsBoolean( v2 ) ;
        		type = "i" ; // integer
            	GetAsNumber( v2 ) ;
        		type = "s" ;
           	 Middle ( v3 ; 2 ; v2 ) ;
        		type = "a" ;
           	 subject ; // not yet supported
        		type = "O" ;
            	subject ; // not yet supported
        		"Unknown serialize format in " &amp; subject

   		 ) // end Case

	) // end Let

; ""

) //end if</Calculation>
      <Comment/>
      <AutoEnter alwaysEvaluate="False"/>
      <Storage storeCalculationResults="True" index="All" indexLanguage="English" global="False" maxRepetition="1"/>
    </Field>
    <Field id="49" dataType="Text" fieldType="Calculated" name="zz_faxpagecount_ct">
      <Calculation table="RC2FM_LOGS">If ( type = "Fax" ; 

	Case ( 

		faxPageCount &gt; 1 and not PatternCount ( messageStatus ; "Failed" ); "( " &amp; faxPageCount &amp; " pages )" ; 

		faxPageCount = 1 and not PatternCount ( messageStatus ; "Failed" ); "( " &amp; faxPageCount &amp; " page )" ;

		"" 

	) //end Case

) //end If</Calculation>
      <Comment/>
      <AutoEnter alwaysEvaluate="False"/>
      <Storage storeCalculationResults="True" autoIndex="True" index="None" indexLanguage="English" global="False" maxRepetition="1"/>
    </Field>
    <Field id="50" dataType="TimeStamp" fieldType="Calculated" name="zz_timeModified_cm">
      <Calculation table="RC2FM_LOGS">Let([

//	_TimeStamp = "2013-10-04T12:13:15";
	
	_year = Left (lastModifiedTime ; 4 );
	_month = Middle ( lastModifiedTime ; 6 ; 2 );
	_day = Middle ( lastModifiedTime ; 9 ; 2 );
	_hour = Middle ( lastModifiedTime ; 12 ; 2 );
	_min = Middle ( lastModifiedTime ; 15 ; 2 );
	_sec = Middle ( lastModifiedTime ; 18 ; 2 )


];


Date ( _month ; _day ; _year ) &amp; " " &amp; Time ( _hour ; _min ; _sec )

)

