    <Field id="51" dataType="Text" fieldType="Calculated" name="zz_recordingduration_ct">
      <Calculation table="RC2FM_LOGS">If ( not IsEmpty ( duration ) ; 

	If ( 

		duration  60 ; "( " &amp; Round ( duration /60 ; 1 ) &amp; " minutes )" ; 

		"( " &amp; duration &amp; " seconds )" 

	 ) //end If

 ) //end if</Calculation>
